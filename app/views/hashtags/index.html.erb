<div class="page-header">
	<h1>Hashtags <small>Index</small></h1>
</div>
<div class="row">
	<div class="span10">
		
		<h2>Latest Hashtags</h2>

		<% @hashtags.each do |hashtag| %>
		<div class="hashtag well">
			<h3>
				<%= link_to hashtag.tag, hashtag_url(hashtag)%>
				<%= link_to "delete", hashtag_url(hashtag,:format => :json), :method=>:delete, :remote => true, :class => "delete btn danger small"%>
			</h3>
		</div>
		<% end %>

		<%= paginate @hashtags %>

	</div>
<%= render "shared/latest_proposals" %>


</div>


